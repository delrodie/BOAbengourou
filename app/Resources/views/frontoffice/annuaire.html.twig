{% extends 'base.html.twig' %}

{% block title %}Liste des entreprises{% endblock %}

{% block stylesheets %}
  {{ parent() }}
{% endblock %}

{% block meta %}
  <meta name="keywords" content="#"/>
  <meta name="description" content="#"/>
  <meta name="revisit" content="3 days"/>
  <meta name="revisit-after" content="3 days">
  <meta name="Author" content="AHOUZOF, Delrodie AMOIKON">
  <meta name="Copyright" content="www.abengourou.ci">
  <meta name="Content-Language" content="FR">
  <meta name="distribution" content="global"/>
  <meta name="Audience" content="All"/>
  <meta name="Robots" content="full">

  <link rel="canonical" href="http://abengourou.ci{{ path('fo_annuaire_liste')}}"/>
  <meta property="og:locale" content="fr_FR"/>
  <meta property="og:type" content="Item"/>
  <meta property="og:title" content="#"/>
  <meta property="og:description" content="#"/>
  <meta property="og:url" content="http://abengourou.ci{{ path('fo_annuaire_liste')}}"/>
  <meta property="og:site_name" content="Abengourou"/>
  <meta property="article:section" content="Item"/>
  <meta property="article:published_time" content="#+00:00"/>
  <meta property="article:modified_time" content="#+00:00"/>
  <meta property="og:updated_time" content="#+00:00"/>
  <meta property="og:image" content="http://abengourou.ci"/>
  <meta name="twitter:card" content="summary"/>
  <meta name="twitter:description" content="#"/>
  <meta name="twitter:title" content="#"/>
  <meta name="twitter:domain" content="Abengourou"/>
  <meta name="twitter:image:src" content="http://abengourou.ci"/>
  <script type="application/ld+json">
    { "@context": "http://schema.org", "@type": "NewsArticle", "mainEntityOfPage": { "@type": "WebPage", "@id": "http://abengourou.ci" }, "headline": "Liste des entreprises", "image": [ "http://abengourou.ci/ressources/images/logos/logo.png", "http://abengourou.ci" ], "datePublished": "#+00:00", "dateModified": "#+00:00", "author": { "@type": "Person", "name": "Delrodie AMOIKON" }, "publisher": { "@type": "Organization", "name": "ABENGOUROU", "logo": { "@type": "ImageObject", "url": "http://abengourou.ci/ressources/images/logos/logo.png" } }, "description": "#" }
  </script>
{% endblock %}

{% block body %}
  <div class="page-title">
    <div class="container">
      <div class="row">
        <div class="col-sm-12">
          <ol class="breadcrumb">
            <li>
              <a href="{{ path('homepage')}}">Accueil</a>
            </li>
            <li>
              <a href="{{ path('fo_annuaire_liste')}}">Annuaire</a>
            </li>
        </ol>
      </div>
    </div>
  </div>
</div>

<section class="block-wrapper">
  <div class="container">
    <div class="row">
      <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">

        <div class="block category-listing category-style2">
          <h3 class="block-title">
            <span>Annuaire</span>
          </h3>

          {% for annuaire in annuaires %}
            <div class="author-box">
              <div class="author-img pull-left">
                <a href="{{ vich_uploader_asset(annuaire, 'imageFile') }}" class="gallery-popup">
                <img src="{{ asset('uploads/annuaire/'~ annuaire.imageName) | imagine_filter('annuaire') }}" alt="{{ annuaire.nom }}" alt="{{ annuaire.nom }}">
                </a>
              </div>
              <div class="author-info">
                <h3>{{ annuaire.nom }}</h3>
                <h5><span style="color: #b70505">{{ annuaire.domaine.nom|upper }}</span>   ({% for tag in annuaire.tag|split(',') %}
                    {{ tag|title }},
                {% endfor %})</h5>
                <p class="author-url">
                  {% if (annuaire.website) %}
                  <a href="http://{{ annuaire.website }}" target="_blank">http://{{ annuaire.website }}</a>
                  {% endif %}
                </p>
                <p>{{ annuaire.quartier }} - {{ annuaire.situation }}</p>
                <p>{{ annuaire.description }}</p>
                <div class="authors-social">
                  <span>Suivre ses activités:
                  </span>
                  {% if (annuaire.facebook) %}
                  <a href="https://facebook.com/{{ annuaire.facebook }}/" target="_blank"><i class="fa fa-facebook-square fa-lg"></i></a>
                  {% endif %}
                  {% if (annuaire.google) %}
                  <a href="https://plus.google.com/{{ annuaire.google }}/" target="_blank"><i class="fa fa-google-plus-square fa-lg"></i></a>
                  {% endif %}
                  {% if (annuaire.twitter) %}
                  <a href="https://twitter.com/{{ annuaire.twitter }}/" target="_blank"><i class="fa fa-twitter-square fa-lg"></i></a>
                  {% endif %}
                  {% if (annuaire.linked) %}
                  <a href="https://linkedin.com/{{ annuaire.linked }}/" target="_blank"><i class="fa fa-linkedin-square fa-lg"></i></a>
                  {% endif %}
                  {% if (annuaire.instagram) %}
                  <a href="https://instagram.com/{{ annuaire.instagram }}/" target="_blank"><i class="fa fa-instagram fa-lg"></i></a>
                  {% endif %}
                  {% if (annuaire.pinterest) %}
                  <a href="https://pinterest.com/{{ annuaire.pinterest }}/" target="_blank"><i class="fa fa-pinterest-square fa-lg"></i></a>
                  {% endif %}
                </div>
              </div>
            </div>
          {% endfor %}

        </div>

        <!--<div class="paging">
          <ul class="pagination">
            <li class="active"><a href="#">1</a></li>
            <li><a href="#">2</a>  </li>
            <li><a href="#">3</a></li>
            <li><a href="#">4</a></li>
            <li><a href="#">»</a></li>
            <li>
              <span class="page-numbers">Page 1 of 2</span>
            </li>
          </ul>
        </div>-->

      </div>

      <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
        <div class="sidebar sidebar-right">
          <div class="widget">
            <h3 class="block-title">
              <span>Nous suivre</span>
            </h3>

            <ul class="social-icon">
              <li>
                <a href="#" target="_blank">
                  <i class="fa fa-rss"></i>
                </a>
              </li>
              <li>
                <a href="#" target="_blank">
                  <i class="fa fa-facebook"></i>
                </a>
              </li>
              <li>
                <a href="#" target="_blank">
                  <i class="fa fa-twitter"></i>
                </a>
              </li>
              <li>
                <a href="#" target="_blank">
                  <i class="fa fa-google-plus"></i>
                </a>
              </li>
              <li>
                <a href="#" target="_blank">
                  <i class="fa fa-vimeo-square"></i>
                </a>
              </li>
              <li>
                <a href="#" target="_blank">
                  <i class="fa fa-youtube"></i>
                </a>
              </li>
            </ul>
          </div>
          <!-- Widget Social end -->

          <div class="widget text-center">
            <img class="banner img-responsive" src="images/banner-ads/ad-sidebar.png" alt=""/>
          </div>
          <!-- Sidebar Ad end -->

          <div class="widget widget-tags">
            <h3 class="block-title">
              <span>Facebook</span>
            </h3>
            <div
              class="fb-page"
              data-href="https://www.facebook.com/Nuit-2-Reves-302256336935002/"
              data-tabs="timeline,events,messages"
              data-height="1200"
              data-small-header="true"
              data-adapt-container-width="true"
              data-hide-cover="false"
              data-show-facepile="true">
              <blockquote cite="https://www.facebook.com/Nuit-2-Reves-302256336935002/" class="fb-xfbml-parse-ignore">
                <a href="https://www.facebook.com/Nuit-2-Reves-302256336935002/">Nuit 2 Reves</a>
              </blockquote>
            </div>
        </ul>
      </div>

    </div>
  </div>

</div>
</div>
</section>
{% endblock %}
