    <a href="{{ asset('uploads/events/'~ menu.imageName) | imagine_filter('actualite') }}" class="gallery-popup"><img class="img-responsive" src="{{ vich_uploader_asset(menu, 'imageFile') | imagine_filter('actualiteMenu') }}" alt="{{ menu.titre }}" /></a>
